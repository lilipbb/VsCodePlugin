{
	"name": "pbb",
	"displayName": "pbb",
	"description": "pbb",
	"publisher":"lilipbb",
	"version": "0.0.1",
	"repository":"",
	"engines": {
		"vscode": "^0.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.nihao"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.nihao",
				"title": "路径"
			}
		],
		"keybindings": [
			{
				"command": "extension.nihao",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.nihao",
					"group": "navigation"
				}
			],
			"explorer/context": [
                {
                    "command": "extension.nihao",
                    "group": "navigation"
                }
            ]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
